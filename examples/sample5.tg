(= mask (perlin octaves:3 persistence:0.1 ))
(= base (perlin))
(= distorted_b (distort mask base (glow)))
(= distorted_b2 (distort mask base base))
(= distorted (| distorted_b))
(= distorted2 (| distorted_b2))
(= one (hsv (+ mask (flat c:0.5)) distorted distorted2))
(= two (rgb (flat) (flat) (flat c:0.5)))
(= out (+3 one two))
